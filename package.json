{
  "name": "service-template",
  "version": "1.0.0",
  "author": "fckffln",
  "description": "Example service template application",
  "license": "MIT",
  "scripts": {
    "preinstall": "environments/security/generate.zsh",
    "postinstall": "bun build:client && bun build:server",
    "start:server": "bun run server/src/main.ts",
    "build:server": "bun build server/src/main.ts --compile",
    "start:client": "cd client && vite && cd ..",
    "build:client": "cd client && vite build && cd ..",
    "start": "nodemon"
  },
  "private": true,
  "dependencies": {
    "luxon": "^3.4.4",
    "mime": "^4.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.1"
  },
  "devDependencies": {
    "@types/luxon": "^3.4.2",
    "@types/mime": "^3.0.4",
    "@types/node": "^20.11.21",
    "@types/react": "^18.2.60",
    "@types/react-dom": "^18.2.19",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "4.2.0",
    "bun-types": "^1.0.29",
    "nodemon": "^3.1.0",
    "vite": "^5.1.4",
    "vite-plugin-mkcert": "^1.17.4",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.1"
  }
}
