{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": [
      "ESNext"
    ],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "moduleResolution": "NodeNext",
    "paths": {
      "@lib/*": ["./lib/*"],
      "@environments": ["../environments"]
    },
  },
}
